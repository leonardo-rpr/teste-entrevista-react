{
  "compilerOptions": {
    "baseUrl": "src",
    "paths": {
      "components": ["app/components"],
      "screens": ["app/screens/"],
      "assets": ["assets"],
      "config": ["config"],
      "forms": ["core/forms"],
      "reducers": ["core/redux/reducers"],
      "sagas": ["core/redux/sagas"],
      "store": ["core/redux/store"],
      "actions": ["core/redux/actions"],
      "api": ["core/api"],
      "apiTypes": ["core/api/types.d.ts"]
    },
    "target": "es5",
    "lib": ["dom", "dom.iterable", "esnext"],
    "allowJs": true,
    "skipLibCheck": true,
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "module": "esnext",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "noEmit": true,
    "jsx": "preserve"
  }
}
